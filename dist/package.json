  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ebony1105/shared-util/issues"
  },
  "homepage": "https://github.com/ebony1105/shared-util#readme",
  "dependencies": {
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "fs-extra": "^11.3.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "workspaces": [
    "@jeanie-testing/shared-util"
