{
  "name": "@jeanie-testing/shared-util",
  "version": "1.0.0",
  "description": "npm publish package demo",
  "main": "index.js",
  "types": "index.d.ts",
  "module": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && node ./build.js",
    "build:dev": "webpack --config webpack.config.js",
